{
    "name": "wplauncher/launcher",
    "description": "The WPLauncher Launcher allows you to turn your Laravel site into a WordPress Hosting company by providing simple ways to interact with WPLauncher's services.",
    "keywords": ["wplauncher", "launcher"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wplauncher/launcher/issues",
        "source": "https://github.com/wplauncher/launcher"
    },
    "authors": [
        {
            "name": "Ben Shadle",
            "email": "benshadle@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/database": "~5.3",
        "illuminate/support": "~5.3",
        "nesbot/carbon": "~1.0",
        "stripe/stripe-php": "~5.0",
        "symfony/http-kernel": "~2.7|~3.0|~4.0"
    },
    "require-dev": {
        "illuminate/http": "~5.3",
        "illuminate/routing": "~5.3",
        "illuminate/view": "~5.3",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Wplauncher\\Launcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wplauncher\\Launcher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "wplauncher": {
            "providers": [
                "Wplauncher\\Launcher\\LauncherServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
